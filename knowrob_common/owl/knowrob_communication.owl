        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/knowrob.owl#Informing"/>
        <rdfs:comment>A specialization of Informing. In each instance of Reporting, some agent imparts collected information to some other agent(s).</rdfs:comment>
        <rdfs:seeAlso>http://sw.opencyc.org/concept/Mx4rwP1IVpwpEbGdrcN5Y29ycA</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/knowrob.owl#Requesting-CommunicationAct -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob.owl#Requesting-CommunicationAct">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/knowrob.owl#CommunicationAct-Single"/>
        <rdfs:comment>A specialization of CommunicationAct_Single. Each instance of Requesting_CommunicationAct is a communication act in which one agent requests something of one or more other agents. Requests are typically followed by responses.</rdfs:comment>
        <rdfs:seeAlso>http://sw.opencyc.org/concept/Mx4rvVjqypwpEbGdrcN5Y29ycA</rdfs:seeAlso>
    </owl:Class>
    


