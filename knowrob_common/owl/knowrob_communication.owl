    <!-- http://knowrob.org/kb/knowrob.owl#Explaining-CommunicationAct -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob.owl#Explaining-CommunicationAct">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/knowrob.owl#Informing"/>
        <rdfs:comment>A specialized collection of communication acts. In each instance of Explaining_CommunicationAct, some agent explains something to some other agent(s). The information given in an act of explaining is an instance of Explanation.</rdfs:comment>
        <rdfs:seeAlso>http://sw.opencyc.org/concept/Mx4rwI_EFpwpEbGdrcN5Y29ycA</rdfs:seeAlso>
    </owl:Class>
    


