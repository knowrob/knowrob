<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [

          <!ENTITY local_path 'file://@OWL_PATH_PREFIX@/owl/'>
          <!ENTITY srdl_path 'file://@SRDL_PATH_PREFIX@/owl/'>
          <!ENTITY rdf 'http://www.w3.org/1999/02/22-rdf-syntax-ns'>
          <!ENTITY rdfs 'http://www.w3.org/2000/01/rdf-schema'>
          <!ENTITY owl 'http://www.w3.org/2002/07/owl'>
          <!ENTITY xsd 'http://www.w3.org/2001/XMLSchema#'>
          <!ENTITY knowrob 'http://ias.cs.tum.edu/kb/knowrob.owl'>
          <!ENTITY actionmodel 'http://ias.cs.tum.edu/kb/actionmodel.owl'>
	  <!ENTITY srdl_capability 'http://ias.cs.tum.edu/kb/SRDL_capability.owl#'>
]> 

<rdf:RDF
    xmlns:computable="http://ias.cs.tum.edu/kb/computable.owl#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns="http://ias.cs.tum.edu/kb/comp_cap.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:am="&actionmodel;#"
    xmlns:knowrob="&knowrob;#"
    xmlns:srdl_capability="http://ias.cs.tum.edu/kb/SRDL_capability.owl#"
    xml:base="http://ias.cs.tum.edu/kb/comp_cap.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="&local_path;knowrob.owl"/>
    <owl:imports rdf:resource="&srdl_path;SRDL.owl"/>
  </owl:Ontology>


    <computable:PrologClass rdf:about="#computeMove_Base">
      <computable:command rdf:datatype="&xsd;string">comp_cap:comp_move_base</computable:command>
      <computable:cache rdf:datatype="&xsd;string">nocache</computable:cache>
      <computable:visible rdf:datatype="&xsd;string">unvisible</computable:visible>
      <computable:target rdf:resource="http://ias.cs.tum.edu/kb/SRDL_capability.owl#move_base"/>
    </computable:PrologClass>   

    <computable:PrologClass rdf:about="#computeMove_Arm">
      <computable:command rdf:datatype="&xsd;string">comp_cap:comp_move_arm</computable:command>
      <computable:cache rdf:datatype="&xsd;string">nocache</computable:cache>
      <computable:visible rdf:datatype="&xsd;string">unvisible</computable:visible>
      <computable:target rdf:resource="http://ias.cs.tum.edu/kb/SRDL_capability.owl#move_arm"/>
    </computable:PrologClass> 
    
    <computable:PrologClass rdf:about="#computeGrasp_Executon">
      <computable:command rdf:datatype="&xsd;string">comp_cap:comp_grasp_execution</computable:command>
      <computable:cache rdf:datatype="&xsd;string">nocache</computable:cache>
      <computable:visible rdf:datatype="&xsd;string">unvisible</computable:visible>
      <computable:target rdf:resource="http://ias.cs.tum.edu/kb/SRDL_capability.owl#grasp_execution"/>
    </computable:PrologClass>

</rdf:RDF>
