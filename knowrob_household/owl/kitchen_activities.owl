        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://knowrob.org/kb/knowrob.owl#inputsCommitted"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://knowrob.org/kb/knowrob.owl#FoodIngredientOnly"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://knowrob.org/kb/knowrob.owl#on-Physical"/>
                                <owl:someValuesFrom rdf:resource="http://knowrob.org/kb/knowrob.owl#SupportingPlane"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://knowrob.org/kb/knowrob.owl#outputsCreated"/>
                <owl:someValuesFrom rdf:resource="http://knowrob.org/kb/knowrob.owl#FoodIngredientOnly"/>
            </owl:Restriction>
