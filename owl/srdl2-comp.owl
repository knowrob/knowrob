        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Gripper"/>
        <rdfs:comment>A gripper that uses magnets to hold metallic objects.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#MappingSoftware -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#MappingSoftware">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#SoftwareType"/>
        <rdfs:comment>Software components that build environment maps from sensor data</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#MaximumSpeed -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#MaximumSpeed">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalAttribute"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#MecanumWheel -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#MecanumWheel">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/knowrob.owl#Wheel"/>
        <rdfs:comment>A wheel that can move a vehicle in any direction on the surface plane.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#MechanicalGripper -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#MechanicalGripper">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Gripper"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#hasPhysicalComponent"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://knowrob.org/kb/knowrob.owl#Finger"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A robot hand with fingers that can wrap around objects.</rdfs:comment>
    </owl:Class>
    


