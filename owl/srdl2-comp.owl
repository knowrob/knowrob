    <!-- http://knowrob.org/kb/knowrob.owl#ElectricalDevice -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob.owl#ElectricalDevice"/>
    


    <!-- http://knowrob.org/kb/knowrob.owl#Finger -->

