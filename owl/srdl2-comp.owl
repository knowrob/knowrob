    <!-- http://knowrob.org/kb/knowrob.owl#MobileBase -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob.owl#MobileBase"/>
    


    <!-- http://knowrob.org/kb/knowrob.owl#SensorDevice -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob.owl#SensorDevice"/>
    


    <!-- http://knowrob.org/kb/knowrob.owl#Wheel -->

    <owl:Class rdf:about="http://knowrob.org/kb/knowrob.owl#Wheel"/>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#Accelerometer -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#Accelerometer">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/knowrob.owl#SensorDevice"/>
        <rdfs:comment>A sensor device that measures acceleration.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#Architecture -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#Architecture">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#BladderGripper -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#BladderGripper">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Gripper"/>
        <rdfs:comment>A gripper designed for rod-shaped or cylindrical objects. Useful to handle fragile objects.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#BluetoothDevice -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#BluetoothDevice">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/srdl2-comp.owl#ComputerHardware"/>
        <rdfs:comment>A device that communicates via bluetooth.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#Classifier -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#Classifier">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#SoftwareType"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#ColorCamera -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#ColorCamera">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/srdl2-comp.owl#LightWaveSensor"/>
        <owl:disjointWith rdf:resource="http://knowrob.org/kb/srdl2-comp.owl#MonochromeCamera"/>
        <rdfs:comment>A sensor device that measures light waves to create a picture where the light waves are encoded in some color space.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#ComputationCore -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#ComputationCore">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/srdl2-comp.owl#ComputerHardware"/>
        <rdfs:comment>A computer processor.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#ComputerHardware -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#ComputerHardware">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/knowrob.owl#ElectricalDevice"/>
        <rdfs:comment>An electircal device designed to be a component in a computer.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#DistanceSensor -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#DistanceSensor">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/knowrob.owl#SensorDevice"/>
        <rdfs:comment>A sensor device that measures the distance to the surface of objects facing the sensor.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#FeatureExtractor -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#FeatureExtractor">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#SoftwareType"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#FieldOfView -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#FieldOfView">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalAttribute"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#FixedHardDrive -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#FixedHardDrive">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/srdl2-comp.owl#HardDrive"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#Gyroscope -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#Gyroscope">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/srdl2-comp.owl#OrientationSensor"/>
        <rdfs:comment>A sensor device that uses gravity to measure orientation. It consists of a freely rotating disk called rotor that is mounted on a spinning axis on a larger wheel.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#HardDrive -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#HardDrive">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/srdl2-comp.owl#MemoryDevice"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#InertialSensor -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#InertialSensor">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/knowrob.owl#SensorDevice"/>
        <rdfs:comment>A sensor device that measures body internal forces.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#InfraredCamera -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#InfraredCamera">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/srdl2-comp.owl#RadiationSensor"/>
        <rdfs:comment>A sensor device that measures infrared radiation to create a picture where the radiation is encoded in some color space (usually with bright colors indicating high radiation).</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#Kinect -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#Kinect">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/srdl2-comp.owl#StereoCamera"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#KinematicChain -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#KinematicChain">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://knowrob.org/kb/srdl2-comp.owl#hasEndLinkName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://knowrob.org/kb/srdl2-comp.owl#hasBaseLinkName"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>Kinematic chain</rdfs:label>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#KnowledgeBase -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#KnowledgeBase">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#SoftwareType"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#LanDevice -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#LanDevice">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/srdl2-comp.owl#NetworkDevice"/>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#LaserScanner -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#LaserScanner">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/srdl2-comp.owl#DistanceSensor"/>
        <rdfs:comment>A sensor device that uses laser beams to measures the distance to the surface of objects facing the sensor.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#LeggedBase -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#LeggedBase">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/knowrob.owl#MobileBase"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#hasPhysicalComponent"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Limb"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A robot base with mechanical limbs that can move the robot by walking.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#LightWaveSensor -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#LightWaveSensor">
        <rdfs:subClassOf rdf:resource="http://knowrob.org/kb/knowrob.owl#SensorDevice"/>
        <rdfs:comment>A sensor device that measures light waves.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knowrob.org/kb/srdl2-comp.owl#LocalizationSoftware -->

    <owl:Class rdf:about="http://knowrob.org/kb/srdl2-comp.owl#LocalizationSoftware">
