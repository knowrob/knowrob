<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY local_path 'file://@IAS_KB_PACKAGE_PATH@/owl/'>
    <!ENTITY srdl_path 'file://@SRDL_PACKAGE_PATH@/owl/'>


    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY swrl "http://www.w3.org/2003/11/swrl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY swrlb "http://www.w3.org/2003/11/swrlb#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY SRDL "http://ias.cs.tum.edu/kb/SRDL.owl#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY knowrob "http://ias.cs.tum.edu/kb/knowrob.owl#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY SRDL_action "http://ias.cs.tum.edu/kb/SRDL_action.owl#" >
    <!ENTITY actionmodel "http://ias.cs.tum.edu/kb/actionmodel.owl#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY SRDL_component "http://ias.cs.tum.edu/kb/SRDL_component.owl#" >
    <!ENTITY SRDL_capability "http://ias.cs.tum.edu/kb/SRDL_capability.owl#" >
]>


<rdf:RDF xmlns="http://ias.cs.tum.edu/kb/SRDL.owl#"
     xml:base="http://ias.cs.tum.edu/kb/SRDL.owl"
     xmlns:actionmodel="http://ias.cs.tum.edu/kb/actionmodel.owl#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:SRDL_component="http://ias.cs.tum.edu/kb/SRDL_component.owl#"
     xmlns:knowrob="http://ias.cs.tum.edu/kb/knowrob.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:SRDL="http://ias.cs.tum.edu/kb/SRDL.owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:SRDL_capability="http://ias.cs.tum.edu/kb/SRDL_capability.owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:SRDL_action="http://ias.cs.tum.edu/kb/SRDL_action.owl#">
    <owl:Ontology rdf:about="">
        <rdfs:comment
            >Main ontology of semlang -  semantic robot description language</rdfs:comment>
        <rdfs:comment
            >Combines and relates elements from the component, task and capability ontologies</rdfs:comment>
        <owl:imports rdf:resource="&local_path;knowrob.owl"/>
        <owl:imports rdf:resource="&srdl_path;SRDL_action.owl"/>
        <owl:imports rdf:resource="&srdl_path;SRDL_capability.owl"/>
        <owl:imports rdf:resource="&srdl_path;SRDL_component.owl"/>
    </owl:Ontology>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&dc;title"/>
    <owl:AnnotationProperty rdf:about="&dc;contributor"/>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->




    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#hasBaseLinkInKinematicChain -->

    <owl:ObjectProperty rdf:about="#hasBaseLinkInKinematicChain">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment
            >Property that is a pointer from a robot to its base link</rdfs:comment>
        <rdfs:domain rdf:resource="#SrdlRobot"/>
        <rdfs:range rdf:resource="&SRDL_component;HardwareComponent"/>
    </owl:ObjectProperty>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#hasCapability -->

    <owl:ObjectProperty rdf:about="#hasCapability">
        <rdfs:comment
            >CAUTION:
This property is only used as an intermediate step to assert capabilities of robots.
Later capabilities should be inferred automatically from robot components!</rdfs:comment>
        <rdfs:domain rdf:resource="#SrdlRobot"/>
        <rdfs:range rdf:resource="&SRDL_capability;PrimitiveCapability"/>
    </owl:ObjectProperty>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#hasCapabilityDependency -->

    <owl:ObjectProperty rdf:about="#hasCapabilityDependency">
        <rdfs:comment
            >Dependency between task t and capability c
Semantics: For task t capability c is necessary/ required</rdfs:comment>
        <rdfs:domain rdf:resource="&SRDL_action;SrdlAction"/>
        <rdfs:range rdf:resource="&SRDL_capability;Capability"/>
    </owl:ObjectProperty>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#hasCapabilityProvisionAlternative -->

    <owl:ObjectProperty rdf:about="#hasCapabilityProvisionAlternative">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment
            >Link between a capability and a capabilityprovisionAlternative
Semantics: The capability can be provided with CapabilityProvisionAlternative</rdfs:comment>
        <owl:deprecated
            >CAUTION: OUTDATED MODELLING
This property is not used in inference anymore.
Instead, its inverse property providesCapability is used.

Reason for model change:
Capability provision alternatives can be added without modifying the SRDL ontologies.</owl:deprecated>
        <rdfs:range rdf:resource="#CapabilityProvisionAlternative"/>
        <rdfs:domain rdf:resource="&SRDL_capability;Capability"/>
    </owl:ObjectProperty>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#hasComponent -->

    <owl:ObjectProperty rdf:about="#hasComponent">
        <rdfs:comment
            >This property between a robot and a component can either be directly asserted or inferred via kinematic chain (property hasSuccessorInKinematicChain)</rdfs:comment>
        <rdfs:domain rdf:resource="#SrdlRobot"/>
        <rdfs:range rdf:resource="&SRDL_component;Component"/>
    </owl:ObjectProperty>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#hasHardwareComponent -->

    <owl:ObjectProperty rdf:about="#hasHardwareComponent">
        <rdfs:domain rdf:resource="#SrdlRobot"/>
        <rdfs:subPropertyOf rdf:resource="#hasComponent"/>
        <rdfs:range rdf:resource="&SRDL_component;HardwareComponent"/>
    </owl:ObjectProperty>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#hasLearningComponent -->

    <owl:ObjectProperty rdf:about="#hasLearningComponent">
        <rdfs:comment
            >Semantics: a capability can be learned with the LearningComponent (given all requirements of the learning component, specified by needsRequirement are fulfilled)</rdfs:comment>
        <rdfs:domain rdf:resource="&SRDL_capability;Capability"/>
        <rdfs:range rdf:resource="&SRDL_component;LearningComponent"/>
    </owl:ObjectProperty>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#hasSoftwareComponent -->

    <owl:ObjectProperty rdf:about="#hasSoftwareComponent">
        <rdfs:domain rdf:resource="#SrdlRobot"/>
        <rdfs:subPropertyOf rdf:resource="#hasComponent"/>
        <rdfs:range rdf:resource="&SRDL_component;SoftwareComponent"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="#hasHardwareComponent"/>
            <rdf:Description rdf:about="&SRDL_component;executesSoftware"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#needsComponent -->

    <owl:ObjectProperty rdf:about="#needsComponent">
        <rdfs:comment
            >Dependency between capability provision alternative  cpa and component comp
Semantic: For capability provision cpa  component comp is necessary</rdfs:comment>
        <rdfs:range rdf:resource="&SRDL_component;Component"/>
    </owl:ObjectProperty>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#providesCapability -->

    <owl:ObjectProperty rdf:about="#providesCapability">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#CapabilityProvisionAlternative"/>
        <rdfs:range rdf:resource="&SRDL_capability;Capability"/>
    </owl:ObjectProperty>



    <!-- http://ias.cs.tum.edu/kb/SRDL_capability.owl#hasSubCapability -->

    <owl:ObjectProperty rdf:about="&SRDL_capability;hasSubCapability"/>



    <!-- http://ias.cs.tum.edu/kb/SRDL_component.owl#executesSoftware -->

    <owl:ObjectProperty rdf:about="&SRDL_component;executesSoftware"/>



    <!-- http://ias.cs.tum.edu/kb/SRDL_component.owl#hasSuccessorInKinematicChain -->

    <owl:ObjectProperty rdf:about="&SRDL_component;hasSuccessorInKinematicChain">
        <rdfs:subPropertyOf rdf:resource="#hasHardwareComponent"/>
    </owl:ObjectProperty>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->




    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#hasNumOfAttempts -->

    <owl:DatatypeProperty rdf:about="#hasNumOfAttempts">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&knowrob;PurposefulAction"/>
        <rdfs:range rdf:resource="&xsd;positiveInteger"/>
    </owl:DatatypeProperty>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#hasNumOfSuccesses -->

    <owl:DatatypeProperty rdf:about="#hasNumOfSuccesses">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&knowrob;PurposefulAction"/>
        <rdfs:range rdf:resource="&xsd;positiveInteger"/>
    </owl:DatatypeProperty>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#hasSuccessProbability -->

    <owl:DatatypeProperty rdf:about="#hasSuccessProbability">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&knowrob;PurposefulAction"/>
        <rdfs:range>
            <rdf:Description>
                <rdf:type rdf:resource="&rdfs;Datatype"/>
                <owl:onDatatype rdf:resource="&xsd;double"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="&xsd;double">1.0</xsd:maxInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="&xsd;double">0.0</xsd:minInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdf:Description>
        </rdfs:range>
    </owl:DatatypeProperty>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#hasValue -->

    <owl:DatatypeProperty rdf:about="#hasValue"/>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->




    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#B21Srdl -->

    <owl:Class rdf:about="#B21Srdl">
        <rdfs:subClassOf rdf:resource="#SrdlRobot"/>
        <rdfs:subClassOf rdf:resource="&knowrob;B21"/>
    </owl:Class>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#BakingFoodRobot -->

    <owl:Class rdf:about="#BakingFoodRobot">
        <rdfs:subClassOf rdf:resource="#SrdlRobotAction"/>
        <rdfs:subClassOf rdf:resource="&knowrob;BakingFood"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasCapabilityDependency"/>
                <owl:someValuesFrom rdf:resource="&SRDL_capability;OperateOvenCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#CapabilityProvisionAlternative -->

    <owl:Class rdf:about="#CapabilityProvisionAlternative">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#providesCapability"/>
                <owl:onClass rdf:resource="&SRDL_capability;Capability"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >One (of possibly several) possibilities to provide a capability</rdfs:comment>
    </owl:Class>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#CarryingWhileLocomotingRobot -->

    <owl:Class rdf:about="#CarryingWhileLocomotingRobot">
        <rdfs:subClassOf rdf:resource="#SrdlRobotAction"/>
        <rdfs:subClassOf rdf:resource="&knowrob;CarryingWhileLocomoting"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasNumOfAttempts"/>
                <owl:hasValue rdf:datatype="&xsd;integer">114</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasNumOfSuccesses"/>
                <owl:hasValue rdf:datatype="&xsd;integer">77</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasCapabilityDependency"/>
                <owl:someValuesFrom rdf:resource="&SRDL_capability;TransportationCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#CommunicateWithAgentAlternative1 -->

    <owl:Class rdf:about="#CommunicateWithAgentAlternative1">
        <rdfs:subClassOf rdf:resource="#PrimitiveCapabilityAlternative"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#needsComponent"/>
                        <owl:someValuesFrom rdf:resource="&SRDL_component;CommunicationComponent"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#needsComponent"/>
                        <owl:someValuesFrom rdf:resource="&SRDL_component;CommunicationMechanism"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#providesCapability"/>
                <owl:someValuesFrom rdf:resource="&SRDL_capability;CommunicateWithAgentCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#CompositeCapabilityAlternative -->

    <owl:Class rdf:about="#CompositeCapabilityAlternative">
        <rdfs:subClassOf rdf:resource="#CapabilityProvisionAlternative"/>
    </owl:Class>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#ControlDeviceAlternative1 -->

    <owl:Class rdf:about="#ControlDeviceAlternative1">
        <rdfs:subClassOf rdf:resource="#PrimitiveCapabilityAlternative"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#needsComponent"/>
                        <owl:someValuesFrom rdf:resource="&SRDL_component;CommunicationComponent"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#needsComponent"/>
                        <owl:someValuesFrom rdf:resource="&SRDL_component;CommunicationMechanism"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#needsComponent"/>
                        <owl:someValuesFrom rdf:resource="&SRDL_component;OtherDeviceControllingComponent"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#providesCapability"/>
                <owl:someValuesFrom rdf:resource="&SRDL_capability;ControlDeviceCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#EggCrackingAlternative1 -->

    <owl:Class rdf:about="#EggCrackingAlternative1">
        <rdfs:subClassOf rdf:resource="#CompositeCapabilityAlternative"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#providesCapability"/>
                <owl:someValuesFrom rdf:resource="&SRDL_capability;EggCrackingCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&SRDL_capability;hasSubCapability"/>
                        <owl:someValuesFrom rdf:resource="&SRDL_capability;PickAndPlaceCapability"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&SRDL_capability;hasSubCapability"/>
                        <owl:someValuesFrom rdf:resource="&SRDL_capability;PressWithForceCapability"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
    </owl:Class>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#HoldAndRotateObjectAlternative1 -->

    <owl:Class rdf:about="#HoldAndRotateObjectAlternative1">
        <rdfs:subClassOf rdf:resource="#PrimitiveCapabilityAlternative"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#needsComponent"/>
                        <owl:someValuesFrom rdf:resource="&SRDL_component;ManipulationEntity"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#needsComponent"/>
                        <owl:someValuesFrom rdf:resource="&SRDL_component;RotatableArm"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#providesCapability"/>
                <owl:someValuesFrom rdf:resource="&SRDL_capability;HoldAndRotateObjectCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#Integer -->

    <owl:Class rdf:about="#Integer">
        <rdfs:subClassOf rdf:resource="#Number-General"/>
    </owl:Class>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#LookBackAlternative1 -->

    <owl:Class rdf:about="#LookBackAlternative1">
        <rdfs:subClassOf rdf:resource="#PrimitiveCapabilityAlternative"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#providesCapability"/>
                <owl:someValuesFrom rdf:resource="&SRDL_capability;LookBackCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#needsComponent"/>
                <owl:someValuesFrom rdf:resource="&SRDL_component;VisualSensorToBack"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#LoweringAnObjectRobot -->

    <owl:Class rdf:about="#LoweringAnObjectRobot">
        <rdfs:subClassOf rdf:resource="#SrdlRobotAction"/>
        <rdfs:subClassOf rdf:resource="&knowrob;LoweringAnObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasNumOfAttempts"/>
                <owl:hasValue rdf:datatype="&xsd;integer">7</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasCapabilityDependency"/>
                <owl:someValuesFrom rdf:resource="&SRDL_capability;PutDownAnObjectCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasNumOfSuccesses"/>
                <owl:hasValue rdf:datatype="&xsd;integer">5</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#MixingByControllingMixer -->

    <owl:Class rdf:about="#MixingByControllingMixer">
        <rdfs:subClassOf rdf:resource="#CompositeCapabilityAlternative"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#providesCapability"/>
                <owl:someValuesFrom rdf:resource="&SRDL_capability;MixingCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&SRDL_capability;hasSubCapability"/>
                        <owl:someValuesFrom rdf:resource="&SRDL_capability;CommunicateWithAgentCapability"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&SRDL_capability;hasSubCapability"/>
                        <owl:someValuesFrom rdf:resource="&SRDL_capability;ControlDeviceCapability"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
    </owl:Class>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#MixingByStirring -->

    <owl:Class rdf:about="#MixingByStirring">
        <rdfs:subClassOf rdf:resource="#CompositeCapabilityAlternative"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#providesCapability"/>
                <owl:someValuesFrom rdf:resource="&SRDL_capability;MixingCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&SRDL_capability;hasSubCapability"/>
                        <owl:someValuesFrom rdf:resource="&SRDL_capability;grasp_execution"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&SRDL_capability;hasSubCapability"/>
                        <owl:someValuesFrom rdf:resource="&SRDL_capability;move_arm"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
    </owl:Class>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#Number-General -->

    <owl:Class rdf:about="#Number-General">
        <rdfs:subClassOf rdf:resource="&knowrob;MathematicalObject"/>
    </owl:Class>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#ObjectRecognitionAlternative1 -->

    <owl:Class rdf:about="#ObjectRecognitionAlternative1">
        <rdfs:subClassOf rdf:resource="#PrimitiveCapabilityAlternative"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#needsComponent"/>
                        <owl:someValuesFrom rdf:resource="&SRDL_component;ObjectRecognitionComponent"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#needsComponent"/>
                        <owl:someValuesFrom rdf:resource="&SRDL_component;PerceptionArchitecture_ObjectPerception"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#needsComponent"/>
                        <owl:someValuesFrom rdf:resource="&SRDL_component;VisualSensor"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#providesCapability"/>
                <owl:someValuesFrom rdf:resource="&SRDL_capability;ObjectRecognitionCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>



    <!-- http://ias.cs.tum.edu/kb/SRDL.owl#OperateOvenAlternative1 -->

    <owl:Class rdf:about="#OperateOvenAlternative1">
        <rdfs:subClassOf rdf:resource="#CompositeCapabilityAlternative"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#providesCapability"/>
                <owl:someValuesFrom rdf:resource="&SRDL_capability;OperateOvenCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&SRDL_capability;hasSubCapability"/>
                        <owl:someValuesFrom rdf:resource="&SRDL_capability;CommunicateWithAgentCapability"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&SRDL_capability;hasSubCapability"/>
                        <owl:someValuesFrom rdf:resource="&SRDL_capability;ControlDeviceCapability"/>
                    </owl:Restriction>
                </owl:intersectionOf>
